{
  "tabWidth": 2,
  "semi": false,
  "singleQuote": false,
  "printWidth": 160,
  "bracketSpacing": true,
  "trailingComma": "none",
  "arrowParens": "always",
  "importOrder": ["^[./]", "^@/(.*)$"],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true,
  "plugins": ["prettier-plugin-tailwindcss"]
}
